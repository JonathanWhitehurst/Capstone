	FROM MasterNames FBS
	inner join TransferOutOff2022AllTeams T
		ON FBS.school = T.origin
		</sql><sql name="Join teams with transfer players">
SELECT PTP.firstName
	,PTP.lastName
	,PTP.positionType
	,PTP.origin
	,PTP.destination
	FROM MasterNames FBS
	inner join PlayerTransferPortal2021 PTP
		ON FBS.school = PTP.destination
		

	</sql><sql name="Compare Team Names from ESPN/CFBD">
SELECT *
	FROM MasterNames FBS
	left join OffPPG2021id ESPN
		ON FBS.TeamFullName = ESPN.Team
	Where ESPN.Team is null

</sql><sql name="UnmatchedTeams">--Allows viewing of team names that do not match, requiring adjust (all corrected now)

